{
  "name": "Emoji replacer",
  "description" : "Replaces emoji in the text with another emoji",
  "version": "1.0",
  "manifest_version": 3,
  "action":{
    "default_popup": "index.html",
    "default_title": "Emoji replacer"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*" , "http://*/*"],
      "js": ["inject.js"]
    }],
    "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs"
    ]
}